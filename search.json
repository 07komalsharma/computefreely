---
layout: null
---
[
	{% for post in site.posts reversed %}
	{
		"title" : "{{ post.title | escape }}",
		"tags" : "{{ post.tags | join: ', ' }}",
		"url" : "{{ post.url }}",
		"logo" : "{{ post.logo }}",
		"category" : "{{ post.category }}",
		"based-on" : "{{ post.notes.based-on }}",
		"install" : "{{ post.notes.install }}"
	}{% unless forloop.last %},{% endunless %}{% endfor %}
]
